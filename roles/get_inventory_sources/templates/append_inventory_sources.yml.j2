###############################################################################
# auto-generated by aap-get-content
# total_inventory_sources: {{ inventory_source_list | length }}
# Ansible Automation Platform: https://{{ aap_hostname }}
# AAP Job ID: {{ tower_job_id }}
# Git User ID: {{ github_user_info.json.login }}
###############################################################################
---
controller_inventory_sources:
{% for inventory_sources in inventory_source_list %}
- name: "{{ inventory_sources.name }}"
{% if inventory_sources.new_name is defined %}
  new_name: "{{ inventory_sources.new_name }}"
{% endif %}
{% if inventory_sources.description is defined %}
  description: {{ inventory_sources.description | default('') | to_json(ensure_ascii=False) }}
{% endif %}
  inventory: "{{ inventory_sources.inventory }}"
  organization: "{{ inventory_sources.organization | default('') }}"
  source: "{{ inventory_sources.source }}"
{% if inventory_sources.source_path is defined %}
  source_path: "{{ inventory_sources.source_path }}"
{% endif %}
{% if inventory_sources.source_vars is defined  and inventory_sources.source_vars != '' and inventory_sources.source_vars != '---' %}
  source_vars:
{% set parsed_variables = inventory_sources.source_vars | default('') | from_yaml %}
    {{ parsed_variables | to_nice_yaml | indent(4) }}{% endif %}
{% if inventory_sources.enabled_var is defined %}
  enabled_var: "{{ inventory_sources.enabled_var }}"
{% endif %}
{% if inventory_sources.enabled_value is defined %}
  enabled_value: {{ inventory_sources.enabled_value }}
{% endif %}
{% if inventory_sources.host_filter is defined %}
  host_filter: "{{ inventory_sources.host_filter }}"
{% endif %}
{% if inventory_sources.limit is defined %}
  limit: {{ inventory_sources.limit }}
{% endif %}
{% if inventory_sources.credentials is defined %}
  credential:
{% for credential in inventory_sources.credentials %}
    - {{ credential }}
{% endfor %}
{% endif %}
{% if inventory_sources.execution_environment is defined %}
  execution_environment: "{{ inventory_sources.execution_environment }}"
{% endif %}
{% if inventory_sources.overwrite is defined %}
  overwrite: {{ inventory_sources.overwrite }}
{% endif %}
{% if inventory_sources.overwrite_vars is defined %}
  overwrite_vars: {{ inventory_sources.overwrite_vars }}
{% endif %}
{% if inventory_sources.custom_virtualenv is defined and inventory_sources.custom_virtualenv %}
  custom_virtualenv: "{{ inventory_sources.custom_virtualenv }}"
{% endif %}
  timeout: {{ inventory_sources.timeout }}
  verbosity: {{ inventory_sources.verbosity }}
{% if inventory_sources.update_on_launch is defined %}
  update_on_launch: {{ inventory_sources.update_on_launch }}
{% endif %}
{% if inventory_sources.update_cache_timeout is defined %}
  update_cache_timeout: {{ inventory_sources.update_cache_timeout }}
{% endif %}
{% if inventory_sources.source_project is defined %}
  source_project: "{{ inventory_sources.source_project }}"
{% endif %}
{% if inventory_sources.scm_branch is defined %}
  scm_branch: "{{ inventory_sources.scm_branch }}"
{% endif %}
  state: {{ inventory_sources.state | default('present') }}
{% if inventory_sources.notification_templates_started is defined %}
  notification_templates_started:
{% for notification_template in inventory_sources.notification_templates_started %}
      - {{ notification_template }}
{% endfor %}
{% endif %}
{% if inventory_sources.summary_fields.notification_templates_success is defined %}
  notification_templates_success:
{% for notification_template in inventory_sources.notification_templates_success %}
      - {{ notification_template }}
{% endfor %}
{% endif %}
{% if inventory_sources.summary_fields.notification_templates_error is defined %}
  notification_templates_error:
{% for notification_template in inventory_sources.notification_templates_error %}
      - {{ notification_template }}
{% endfor %}
{% endif %}

{% endfor %}